[
  {
    "name": "${name}",
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "memory": 512,
    "cpu": 256,
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "recipe",
        "awslogs-region": "eu-west-1",
        "awslogs-stream-prefix": "${name}"
      }
    },
    "environment": [

    ]

  }
]
